{
  "name": "linkwave",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 4000",
    "dev:turbo": "next dev --turbo -p 4000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lintfix": "npx lint-staged && eslint src/**/*.ts --fix && next lint",
